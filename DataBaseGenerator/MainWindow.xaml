<Window x:Class="DataBaseGenerator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="500" Width="900" WindowState="Maximized">
    <Grid>
        <Label Content="Database generator" HorizontalAlignment="Left" Margin="25,10,0,0" VerticalAlignment="Top" Width="164"/>
        <Label Content="Select Database Schema" HorizontalAlignment="Left" Margin="25,62,0,0" VerticalAlignment="Top"/>
        <Button x:Name="LoadXMLDataBaseSchema" Content="Load" HorizontalAlignment="Left" Margin="430,65,0,0" VerticalAlignment="Top" Width="75" Click="LoadXMLDataBaseSchema_Click"/>
        <TextBox x:Name="XmlFilePath" HorizontalAlignment="Left" Height="23" Margin="169,65,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="241" IsEnabled="False"/>
        <Button x:Name="GenerateObjectsDescription" Content="Generate" HorizontalAlignment="Left" Margin="521,65,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.127,0.597" Click="GenerateObjectsDescription_Click"/>
        <StackPanel HorizontalAlignment="Left" Height="298" Margin="25,131,0,0" VerticalAlignment="Top" Width="279"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="356,203,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
